<https://homernetwork.github.io/vocabulary/audiences/> dct:description "Terms to describe audiences."@en;
  dct:title "Audiences"@en .

audiences:audience_per_age_category skos:prefLabel "audience per age category"@en .

audiences:adults skos:broader audiences:audience_per_age_category;
  skos:definition "A person who is fully grown or developed."@en;
  skos:prefLabel "adults"@en .

audiences:children skos:broader audiences:audience_per_age_category;
  skos:definition "A young person below the age of majority"@en;
  skos:prefLabel "children"@en .

audiences:teenager skos:definition "A young person in the age range of 12-19"@en;
  skos:prefLabel "teenager"@en .

audiences:young_adults skos:definition "An adult in the age range of approximately 18-25"@en;
  skos:prefLabel "young adults"@en .

audiences:senior_citizens skos:altLabel "elderly"@en;
  skos:broader audiences:audience_per_age_category;
  skos:definition "An older adult of retirement aga and above"@en;
  skos:prefLabel "senior citizens"@en .

audiences:audience_per_gender skos:definition "Audiences defined by their gender self-definition"@en;
  skos:editorialNote "sensitive"@en;
  skos:prefLabel "audience per gender"@en .

audiences:female skos:altLabel "women"@en;
  skos:broader audiences:audience_per_gender;
  skos:definition "Audiences who self-identify as female"@en;
  skos:editorialNote "sensitive"@en;
  skos:prefLabel "female"@en .

audiences:male skos:altLabel "men"@en;
  skos:broader audiences:audience_per_gender;
  skos:definition "Audience who self-identify as male"@en;
  skos:editorialNote "sensitive"@en;
  skos:prefLabel "male"@en .

audiences:non-binary skos:broader audiences:audience_per_gender;
  skos:definition "Audiences who self-identify as non-binary"@en;
  skos:editorialNote "sensitive"@en;
  skos:prefLabel "non-binary"@en .

audiences:trans skos:broader audiences:audience_per_gender;
  skos:definition "Audiences who self-identify as trans"@en;
  skos:editorialNote "sensitive"@en;
  skos:prefLabel "trans"@en .

audiences:other skos:broader audiences:audience_per_gender;
  skos:definition "Audiences who self-identify as other than male, female, non-binary or trans"@en;
  a skos:Concept;
  skos:editorialNote "sensitive"@en;
  skos:prefLabel "other"@en .

audiences:audience_per_sexual_orientation a skos:Concept;
  skos:definition "Audiences who define themselves based on their sexual orientation"@en;
  skos:editorialNote "sensitive"@en;
  skos:prefLabel "audience per sexual orientation"@en .

audiences:audience_per_ethnicity a skos:Concept;
  skos:definition "Audiences who define themselves or are defined by others based on their ethnicity"@en;
  skos:editorialNote "sensitive"@en;
  skos:prefLabel "audience per ethnicity"@en .

audiences:audience_per_nationality a skos:Concept;
  skos:definition "Audiences who define themselves in relation to one or more national identities"@en;
  skos:editorialNote "sensitive"@en;
  skos:prefLabel "audience per nationality"@en .

audiences:audience_per_region a skos:Concept;
  skos:definition "Audiences who define themselves in relation to regional identity"@en;
  skos:prefLabel "audience per region"@en .

audiences:audience_per_language a skos:Concept;
  skos:definition "Audiences who define themselves in relation to one or more languages"@en;
  skos:prefLabel "audience per language"@en .

audiences:audience_per_ideology a skos:Concept;
  skos:definition "Audiences who define themselves or are defined by others in relation to one or more ideologies"@en;
  skos:prefLabel "audience per ideology"@en .

audiences:audience_per_religion a skos:Concept;
  skos:definition "Audiences who define themselves or are defined by others in relation to one or more religions"@en;
  skos:prefLabel "audience per religion"@en .

audiences:audience_per_political_orientation a skos:Concept;
  skos:definition "Audiences who define themselves or are defined by others in relation to one or more political orientations"@en;
  skos:prefLabel "audience per political orientation"@en .

audiences:audience_per_household_income a skos:Concept;
  skos:definition "Audiences defined by household income"@en;
  skos:prefLabel "audience per household income"@en .

audiences:low_income a skos:Concept;
  skos:broader audiences:audience_per_household_income;
  skos:definition "Audiences defined as belonging to a low-income group"@en;
  skos:prefLabel "low income"@en .

audiences:middle_income a skos:Concept;
  skos:broader audiences:audience_per_household_income;
  skos:definition "Audiences defined as belonging to a middle-income group"@en;
  skos:prefLabel "middle income"@en .

audiences:high_income a skos:Concept;
  skos:broader audiences:audience_per_household_income;
  skos:definition "Audiences defined as belonging to a high-income group"@en;
  skos:prefLabel "high income"@en .

audiences:audience_per_social_class a skos:Concept;
  skos:definition "Audiences who identify or are identified as belonging to a particular social class"@en;
  skos:narrower audiences:elite_social_class, audiences:middle_social_class, audiences:working_social_class;
  skos:prefLabel "audience per social class"@en .

audiences:working_social_class a skos:Concept;
  skos:broader audiences:audience_per_social_class;
  skos:definition "Audiences who identify or are identified as belonging to the working class"@en;
  skos:prefLabel "working social class"@en .

audiences:middle_social_class a skos:Concept;
  skos:broader audiences:audience_per_social_class;
  skos:definition "Audiences who identify or are identified as belonging to the middle class"@en;
  skos:prefLabel "middle social class"@en .

audiences:elite_social_class a skos:Concept;
  skos:broader audiences:audience_per_social_class;
  skos:definition "Audiences who identify or are identified as belonging to the elite class"@en;
  skos:prefLabel "elite social class"@en .

audiences:audience_per_education a skos:Concept;
  skos:definition "Audiences defined by level of edication"@en;
  skos:prefLabel "audience per education"@en .

audiences:audience_per_occupation a skos:Concept;
  skos:definition "Audiences defined by occupation"@en;
  skos:narrower audiences:firemen, audiences:military_audience;
  skos:prefLabel "audience per occupation"@en .

audiences:military_audience a skos:Concept;
  skos:broader audiences:audience_per_occupation;
  skos:definition "Audiences defined as belonging to the military"@en;
  skos:prefLabel "military audience"@en .

audiences:firemen a skos:Concept;
  skos:broader audiences:audience_per_occupation;
  skos:definition "Audiences defined as working as firemen"@en;
  skos:prefLabel "firemen"@en .

audiences:biografpublik a skos:Concept;
  skos:broader audiences:cinema_audience;
  skos:definition "Audience that goes to the movies to watch film"@en;
  skos:prefLabel "Biografpublik"@sv, "biografpublik"@en .

audiences:film_viewer a skos:Concept;
  skos:broader audiences:film_audience;
  skos:definition "An individual watching a film"@en;
  skos:prefLabel "film viewer"@en .

audiences:film_audience a skos:Concept;
  skos:definition "A body of individuals watching and/or listening to a show, concert, film, or speech. The cinema audience comprises people who assemble to watch films in cinemas and other venues, both public and private, as well as those who consume films via alternative platforms such as video, DVD, home cinema, and television."@en;
  skos:narrower audiences:film_viewer, audiences:filmpublik, audiences:pubblico, audiences:spettatore;
  skos:inScheme <https://homernetwork.github.io/vocabulary/audiences/>;
  skos:prefLabel "film audience"@en .

audiences:cinema_audience a skos:Concept;
  skos:definition "They are the sum of individuals who have bought a cinema ticket in a particular location/locality during a specific (period of) time."@en;
  skos:inScheme <https://homernetwork.github.io/vocabulary/audiences/>;
  skos:narrower audiences:biografpublik, audiences:cinemagoers;
  skos:prefLabel "cinema audience"@en .

audiences:cinemagoers a skos:Concept;
  skos:broader audiences:cinema_audience;
  skos:definition "Individuals connected to the cinema. Not as simple as simply saying 'cinemagoers' although this is a preferred term. But also including those who may not be attending, but still connected to the cinema through physical proximity (they pass a marquee) or through social media/online marketing and comms"@en;
  skos:editorialNote "The current definition seems like it is defining another word (since it says it is not as simple as calling them cinemagoers)."@en;
  skos:inScheme <https://homernetwork.github.io/vocabulary/audiences/>;
  skos:prefLabel "cinemagoers"@en .

audiences:filmpublik a skos:Concept;
  skos:broader audiences:film_audience;
  skos:definition "Film audience (regardless of which kind of screen the film is shown on). Doesn't necessarily have to be on a regular basis"@en;
  skos:inScheme <https://homernetwork.github.io/vocabulary/audiences/>;
  skos:prefLabel "Filmpublik"@sv, "filmpublik"@en .

audiences:pubblico a skos:Concept;
  skos:broader audiences:film_audience;
  skos:definition "The group of people who frequent a place open to the public, e.g. shops, offices, theatres, libraries or general of entertainment"@en;
  skos:inScheme <https://homernetwork.github.io/vocabulary/audiences/>;
  skos:prefLabel "Pubblico"@it, "pubblico"@en .

audiences:spettatore a skos:Concept;
  skos:broader audiences:film_audience;
  skos:definition "A person who takes part in a show, a ceremony or a sporting event [definition, used most frequently in the plural form \"spettatori\"]"@en;
  skos:inScheme <https://homernetwork.github.io/vocabulary/audiences/>;
  skos:prefLabel "Spettatore"@it, "spettatore"@en .

